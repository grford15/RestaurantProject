<p class="pageHeader">Bookings On This Day</p>

<table class="table">
    <tr>
        <th>Date and Time</th>
        <th>Customer</th>
        <th>Table Seating</th>
        <th>No in Party</th>
    </tr>
    #foreach($booking in $bookings)
    <tr>
        <td>$booking.prettyTimeDate()</td>
        <td>$booking.getCustomer().getName()</td>
        <td>$booking.getSeating().getChairs()</td>
        <td>$booking.getCapacity()</td>
    </tr>
#end
</table>

<a href="/bookings" class="cust-button">Back to Bookings</a>